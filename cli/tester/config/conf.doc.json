{
    //Contient la liste des environements de test disponibles
    "environments" : {
        "name" : "full\\class\\Name"
    },
    //Contient la liste des launchers de tests disponibles
    "launchers" : {
        "name" : "full\\class\\Name"
    },
    //Liste des sequences de tests disponibles.
    //Chaque séquence est définie dans un launcher particulier.
    //Chaque sequence est lancée dans un environnement séparé des autres séquences (nouveau processus)
    //Si des environnements de test sont définis dans une séquence, ils seront utilisés tour à tour dans des séquences
    //de tests séparées.
    "sequences" : {
        //Nom du launcher utilisé pour les sequences
        "launcherName" : {
            //Type de test (integration,unit,...)
            "type" : {
                //Nom de la séquence
                "name" : {
                    //(optionnel) Description de la séquence
                    "description" : "Test sequence description",
                    //(optionnel) Liste des environnements à utiliser pour la séquence de tests (surtout pous les tests d'intégration)
                    "environments" : ["environmentName1","environmentName2"],
                    //Chemins vers les dossiers contenant les tests.
                    //Les chemins relatifs sont résolus suivant ROOT."/tests/{launcherName}/{testType}/{relativePath}
                    "paths" : [
                        "path/to/test/folder",
                        "path/to/another/test/folder2"
                    ]
                }
            }
        }
    }
}