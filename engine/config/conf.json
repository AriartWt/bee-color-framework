{
	"server": {
		"framework": {
			"name": "wfw",
			"version": "STABLE-1.2"
		},
		"daemons": {
			"kvs": "kvstore\/server\/config\/conf.json",
			"model_supervisor": "modelSupervisor\/server\/config\/conf.json",
			"sctl": "sctl\/config\/conf.json"
		},
		"cli": {
			"backup": "backup\/config\/conf.json",
			"updator": "updator\/config\/conf.json"
		},
		"language": {
			"default": "fr",
			"availables": [
				"fr"
			]
		},
		"router": {
			"connections": []
		},
		"tmp": {
			"dir": "site\/tmp"
		},
		"uploader" : {
			"dir" : "site/webroot/uploads",
			"quotas" : "1Go",
			"max_size" : "300Mo",
			"accepted_mimes" : ["/^image\\/.*$/","/^video\\/mp4$/","/^audio\\/mp3$/","/^audio\\/mpeg$/"]
		},
	  	"module" : {
		  	"users" : {
			  	"cgu_link" : "cgu",
			  	"register_type" : "client"
			}
		},
		"display_loading_time": false
	},
	"app": {
		"dev": false,
		"params": {
			"ui": {
				"notifications": {
					"desktopNotifications": true,
					"displayTime": 10000,
					"xStart": "right",
					"yStart": "bottom",
					"xShift": "0",
					"yShift": "0"
				},
				"CssSlider": {
					"_PLAY_SPEED": 6000,
					"_AUTORUN_AFTER": 30000
				},
				"lang": {
					"replacement_pattern": "[$]"
				}
			},
			"EVENTS": {
				"ClickNDbl": {
					"_DELAY": 300
				}
			}
		}
	}
}